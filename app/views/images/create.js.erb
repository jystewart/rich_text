<html>
<body>
  <script type='text/javascript' charset='utf-8'>
  var loc = document.location;
  with(window.parent) { 
    setTimeout(function() { 
      window.eval("SHEBA.Images.display_options('<%= @image.upload.url(:medium) %>', '<%= @image.upload.url(:small) %>', '<%= @image.upload.url(:thumb) %>', '<%= @image.caption %>', '<%= @image.attribution %>', '<%= @image.link %>')"); 
      loc.replace('about:blank'); 
    }, 1) 
  } 
  </script>
</body>
</html>